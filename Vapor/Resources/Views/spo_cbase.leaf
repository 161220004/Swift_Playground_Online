<!DOCTYPE html>
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
	<title>#get(title)</title>

  <!-- CSS  -->
  <!-- My CSS -->
	<link rel="stylesheet" href="/css/view.css">
	<link rel="stylesheet" href="/css/fix.css">
  <!-- Import Google Icon Font -->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!-- Import materialize.css -->
  <link type="text/css" rel="stylesheet" href="/css/materialize.min.css"  media="screen,projection"/>
</head>
<body>
  <!-- 导航栏 -->
  <nav class="indigo lighten-3 my-nav-height-large" role="navigation">
    <div class="nav-wrapper">
      <a id="logo-container" href="#" class="center brand-logo">Swift Playground Online</a>
      <ul class="right hide-on-med-and-down">
        <li><a href="/spo/welcome">首页</a></li>
      </ul>
      <ul id="nav-mobile" class="sidenav">
        <li class="center-align indigo lighten-3"><h3 style="margin: 0px; padding: 16px 8px;">GoTo</h3></li>
        <li><a href="/spo/welcome">首页</a></li>
      </ul>
      <a href="#" data-target="nav-mobile" class="sidenav-trigger">
        <i class="material-icons my-fix-menu">menu</i>
      </a>
    </div>
  </nav>

  <!-- Space Line -->
  <div class="my-space-line-large"></div>

  <!-- 标题 -->
  <h3 class="center indigo-text text-darken-4">#get(chapter_title)</h3>

  <!-- Space Line -->
  <div class="my-space-line-large"></div>

  <!-- 介绍 -->
  <div class="container indigo-text text-darken-4">#get(chapter_introduction)</div>

  <!-- Space Line -->
  <div class="my-space-line-large"></div>

  <!-- 欢迎词 -->
  <div class="center indigo-text text-darken-4"><h5>#get(chapter_welcome)</h5></div>

  <!-- Space Line -->
  <div class="my-space-line-large"></div>

  <div class="row container center">#get(chapter_start)</div>

  <!-- Space Line -->
  <div class="my-space-line-huge"></div>

  <!-- 关卡选择 -->
  <h5 class="center indigo-text text-darken-4">关卡选择</h5>
  <div class="my-space-line"></div>
  <div class="row container center">#get(chapter_puzzles)</div>

  <!-- Puzzle目录按钮集 -->
  <div class="fixed-action-btn">
    <a class="btn-floating btn-large brown">
      <i class="large material-icons">attach_file</i>
    </a>
    <ul>
      <li><a class="btn-floating red" href="/spo/chapter/1">壹</a></li>
      <li><a class="btn-floating yellow darken-1" href="/spo/chapter/2">贰</a></li>
      <li><a class="btn-floating green" href="/spo/chapter/3">叁</a></li>
      <li><a class="btn-floating blue" href="/spo/chapter/4">肆</a></li>
      <li><a class="btn-floating purple" href="/spo/chapter/5">伍</a></li>
    </ul>
  </div>

  <!-- Space Line -->
  <div class="my-space-line-large"></div>

  <!-- My Footer -->
  <footer class="page-footer indigo lighten-3">
    <div class="container">
      <div class="row">
        <div class="col l6 m6 s12">
          <h5 class="white-text">Footer Content</h5>
          <p class="grey-text text-lighten-4">You can use rows and columns here to organize your footer content.</p>
        </div>
        <div class="col l4 offset-l2 s12">
          <h5 class="white-text">Links</h5>
          <ul>
            <li><a class="grey-text text-lighten-3" href="#!">Link 1</a></li>
            <li><a class="grey-text text-lighten-3" href="#!">Link 2</a></li>
          </ul>
        </div>
      </div>
    </div>
    <div class="footer-copyright indigo lighten-2">
      <div class="container">
        © 2014 Copyright AldebaRain
        <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
      </div>
    </div>
  </footer>

  <!--JavaScript at end of body for optimized loading-->
  <!-- jQuery -->
  <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
  <!-- For Materialize -->
  <script src="/js/Materialize/materialize.min.js" type="text/javascript"></script>
  <script src="/js/Materialize/initm.js" type="text/javascript"></script>
  <!-- For Chapter Addition -->
  #get(chapter_js)
</body>
</html>
